{
    "name": "bgg-games-ranks-parser",
    "version": "0.1.0",
    "description": "bgg games ranks parser",
    "main": "index.js",
    "engines": {
        "node": ">=18.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "jest --config jestconfig.json --coverage",
        "build": "rm -rf dist && tsc",
        "dev": "npm run build && npx @google-cloud/functions-framework --target=main --source=dist",
        "deploy": "npm run build && cp package*.json dist && gcloud functions deploy bgg-games-ranks-parser --gen2 --trigger-http --runtime nodejs22 --entry-point=main --no-allow-unauthenticated --region=europe-central2 --source=dist --update-build-env-vars=\"GOOGLE_NODE_RUN_SCRIPTS=\" --project zinovik-project"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "5.0.2",
        "@sparticuz/chromium": "143.0.4",
        "puppeteer": "24.37.5",
        "xmldom": "0.6.0",
        "xpath": "0.0.34"
    },
    "devDependencies": {
        "@types/jest": "30.0.0",
        "@types/xmldom": "0.1.34",
        "jest": "30.2.0",
        "ts-jest": "29.4.6",
        "typescript": "5.9.3"
    },
    "author": "Max Zinovik",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zinovik/bgg-games-ranks-parser.git"
    },
    "bugs": {
        "url": "https://github.com/zinovik/bgg-games-ranks-parser/issues"
    },
    "homepage": "https://github.com/zinovik/bgg-games-ranks-parser#readme"
}
