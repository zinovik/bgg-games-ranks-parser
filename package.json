{
    "name": "bgg-games-ranks-parser",
    "version": "0.1.0",
    "description": "bgg games ranks parser",
    "main": "index.js",
    "engines": {
        "node": ">=18.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "jest --config jestconfig.json --coverage",
        "build": "rm -rf dist && tsc",
        "dev": "tsc --watch & npx @google-cloud/functions-framework --target=main --source=dist --watch",
        "deploy": "npm run build && cp package*.json dist && gcloud functions deploy bgg-games-ranks-parser --gen2 --trigger-http --runtime nodejs20 --entry-point=main --region=us-central1 --source=dist --update-build-env-vars=\"GOOGLE_NODE_RUN_SCRIPTS=\" --project zinovik-project",
        "service:account": "gcloud iam service-accounts keys create key-file.json --iam-account=zinovik-project@appspot.gserviceaccount.com",
        "describe": "gcloud functions describe bgg-games-ranks-parser --region=us-central1"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "3.3.0",
        "axios": "1.6.2",
        "xmldom": "0.6.0",
        "xpath": "0.0.32"
    },
    "devDependencies": {
        "@types/xmldom": "0.1.34",
        "typescript": "5.3.3",
        "jest": "29.0.3",
        "@types/jest": "29.0.1",
        "ts-jest": "29.0.0"
    },
    "author": "Max Zinovik",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zinovik/bgg-games-ranks-parser.git"
    },
    "bugs": {
        "url": "https://github.com/zinovik/bgg-games-ranks-parser/issues"
    },
    "homepage": "https://github.com/zinovik/bgg-games-ranks-parser#readme"
}
